{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": "3.6",
        "targets": "> 0.25%, not dead"
      }
    ],
    [
      "@babel/preset-react",
      {
        "development": true
      }
    ],
    "@emotion/babel-preset-css-prop",
    "@babel/preset-typescript",
    "minify"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "^@internal/(.+)$": "./@internal/\\1",
          "^@assets/(.+)$": "./assets/\\1"
        }
      }
    ],
    "import-graphql",
    "annotate-console-log"
  ]
}